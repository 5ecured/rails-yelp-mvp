<%= simple_form_for [ @restaurant, @review ] do |f| %>
  <%= f.error_notification %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.button :submit, value: "Post review", class: "btn btn-primary" %>
<% end %>


<%= render '/restaurants/goback' %> <!--because the partial is in the restaurants folder -->
